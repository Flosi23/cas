{
  "name": "cas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s next:build",
    "check": "tsc --noEmit",
    "dev": "cross-env NODE_ENV=development run-s next:dev",
    "export": "cross-env NODE_ENV=production run-s next:export",
    "production": "cross-env NODE_ENV=production run-s build start",
    "start": "cross-env NODE_ENV=production npm next:start",
    "fix": "run-s prettier:fix eslint:fix",
    "lint": "run-s prettier:report eslint:report",
    "next:build": "next build",
    "next:dev": "next dev",
    "next:export": "next export",
    "next:start": "next start",
    "prettier:fix": "npm prettier:individual -- .",
    "prettier:individual": "npm prettier:template -- --write",
    "prettier:report": "npm prettier:template -- --check .",
    "prettier:template": "prettier --plugin-search-dir=. --ignore-unknown",
    "eslint:fix": "npm eslint:template -- --fix .",
    "eslint:individual": "npm eslint:template -- --fix",
    "eslint:report": "npm eslint:template -- --max-warnings 0 .",
    "eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "framer-motion": "^6.2.8",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "cross-env": "^7.0.3",
    "eslint": "8.10.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-react-hooks": "4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "prettier-plugin-pkg": "0.11.1",
    "prettier-plugin-sort-json": "0.0.2",
    "typescript": "4.6.2"
  }
}
